{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Sweden: Population Density by County (Län)",
    "subtitle": "Inhabitants per km², 2025",
    "fontSize": 16,
    "anchor": "start",
    "subtitleFontSize": 12
  },
  "width": "container",
  "height": "container",
  "data": {
    "url": "https://raw.githubusercontent.com/kelvinchwng/kelvinchwng.github.io/main/portfolio/data/se.json",
    "format": {"property": "features"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/kelvinchwng/kelvinchwng.github.io/main/portfolio/data/cc5_data2.csv"
        },
        "key": "name",
        "fields": ["density"]
      }
    }
  ],
  "projection": {"type": "mercator"},
  "mark": {
    "type": "geoshape",
    "stroke": "white",
    "strokeWidth": 0.5
  },
  "encoding": {
    "color": {
      "field": "density",
      "type": "quantitative",
      "scale": {
        "type": "log",
        "scheme": "blues"
      },
      "legend": {
        "title": "Density (per km²)",
        "format": ".0f"
      }
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "County"},
      {"field": "density", "type": "quantitative", "title": "Density", "format": ".1f"}
    ]
  }
}
