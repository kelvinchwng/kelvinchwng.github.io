{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Population density of Sweden's 290 municipalities - data scraped from Wikipedia",
  "width": "container",
  "height": 450,
  "title": {
    "text": "Sweden: Population Density by Municipality",
    "subtitle": "People per km². Source: Wikipedia (scraped)",
    "anchor": "start",
    "subtitleFontSize": 11,
    "subtitleColor": "#666"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/okfse/sweden-geojson/master/swedish_municipalities.geojson",
    "format": {"type": "json", "property": "features"}
  },
  "transform": [
    {
      "lookup": "properties.kom_namn",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/kelvinchwng/kelvinchwng.github.io/main/portfolio/data/cc5_data2.csv"
        },
        "key": "municipality",
        "fields": ["density", "code"]
      }
    }
  ],
  "projection": {"type": "mercator"},
  "mark": {
    "type": "geoshape",
    "stroke": "#fff",
    "strokeWidth": 0.3
  },
  "encoding": {
    "color": {
      "field": "density",
      "type": "quantitative",
      "scale": {
        "type": "log",
        "domain": [0.1, 100, 7000],
        "range": ["#f7fbff", "#6baed6", "#08306b"]
      },
      "legend": {
        "title": "Density (per km²)",
        "orient": "bottom",
        "direction": "horizontal",
        "gradientLength": 200
      }
    },
    "tooltip": [
      {"field": "properties.kom_namn", "type": "nominal", "title": "Municipality"},
      {"field": "density", "type": "quantitative", "title": "Density (per km²)", "format": ".1f"}
    ]
  },
  "config": {
    "view": {"stroke": "transparent"},
    "font": "Inter, system-ui, sans-serif"
  }
}